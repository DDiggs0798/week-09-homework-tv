html
  include partials/head.pug

  body
    h1= showdata.title

    h2 Developed By
    ul
      for developedBy in showdata.developedBy
        li= developedBy

    h2 Starring
    ul
      for starring in showdata.starring
        li= starring

    h2 Seasons
    ul
      for season in showdata.seasons
        li
          a(href=`/season/${season.number}`)= `Season ${season.number}`
